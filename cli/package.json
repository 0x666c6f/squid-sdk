{
  "name": "@subsquid/cli",
  "description": "squid cli tool",
  "version": "0.0.0",
  "license": "MIT",
  "bin": {
    "squid": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib",
    "!/lib/test",
    "/resource",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "squid",
    "scope": "@subsquid",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/errors"
    ]
  },
  "homepage": "https://www.subsquid.io/",
  "scripts": {
    "build": "rm -rf lib && tsc && oclif-dev manifest",
    "test": "nyc mocha --forbid-only \"lib/test/**/*.test.js\""
  },
  "dependencies": {
    "@inquirer/input": "^0.0.13-alpha.0",
    "@inquirer/password": "^0.0.12-alpha.0",
    "@inquirer/select": "^0.0.13-alpha.0",
    "@oclif/command": "^1.8.12",
    "@oclif/config": "^1.18.2",
    "@oclif/errors": "^1.3.5",
    "@oclif/plugin-help": "^2",
    "@octokit/auth-oauth-device": "^3.1.2",
    "@subsquid/openreader": "^0.2.1",
    "@subsquid/util": "^0.0.0",
    "chalk": "^4.1.2",
    "cli-select": "^1.1.2",
    "cli-ux": "^5.6.3",
    "dotenv": "^10.0.0",
    "execa": "^4.1.0",
    "node-fetch": "^2.6.1",
    "pgtools": "^0.3.2",
    "query-string": "^7.0.1",
    "simple-git": "^2.44.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.9",
    "@oclif/test": "^1.2.9",
    "@polkadot/types": "^6.4.2",
    "@subsquid/substrate-processor": "^0.0.0",
    "@types/node-fetch": "^2.5.12",
    "nock": "^13.1.3",
    "typeorm": "^0.2.41",
    "typescript": "~4.5.3"
  }
}
